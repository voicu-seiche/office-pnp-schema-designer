        "redux-thunk": "2.3.0",
        "xml2js": "0.4.19"
    },
    "devDependencies": {
        "@types/history": "4.7.0",
        "@types/node": "10.7.1",
        "@types/prop-types": "15.5.5",
        "@types/react": "16.4.11",
        "@types/react-dom": "16.0.7",
        "@types/react-redux": "6.0.6",
